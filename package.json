{
  "name": "be-book",
  "version": "1.0.0",
  "description": "",
  "author": "leceng",
  "private": true,
  "license": "UNLICENSED",
  "_moduleAliases": {
    "libs": "dist/libs",
    "apps": "dist/apps"
  },
  "prisma": {
    "seed": "ts-node ./libs/modules/prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.{ts, js, json}\" \"libs/**/*.{ts, js, json}\" ",
    "start:dev": "nest start @app/api-core --debug --watch",
    "start:prd": "node dist/apps/api-core/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:coverage": "jest --coverage --passWithNoTests --collectCoverageFrom='src/core/middlewares/apiSignature'",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=apps/base/test jest --config ./test/jest-e2e.json"
  },
  "schema": "libs/prisma/schema.prisma",
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^6.1.0",
    "@willsoto/nestjs-prometheus": "^5.1.2",
    "apple-signin-auth": "^1.7.6",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "bull": "^4.13.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "colorette": "^2.0.20",
    "concurrently": "^7.6.0",
    "convert-pino-request-to-curl": "^0.0.29",
    "dotenv": "8.6.0",
    "express-prom-bundle": "^6.6.0",
    "fb": "^2.0.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^9.11.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "libphonenumber-js": "^1.11.3",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "migrate": "^2.0.0",
    "module-alias": "^2.2.3",
    "nestjs-cls": "^4.3.0",
    "openai": "^4.52.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1",
    "prisma": "^5.20.0",
    "prom-client": "^15.1.3",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "round-robin-js": "^3.0.5",
    "rxjs": "^7.2.0",
    "systeminformation": "^5.18.8",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.13",
    "@types/fb": "^0.0.28",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.14.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.151.1",
    "tsconfig-paths": "4.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@lib/lib(|/.*)$": "<rootDir>/libs/lib/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1"
    }
  }
}
